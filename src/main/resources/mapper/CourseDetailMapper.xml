            <result property="start_school_year" column="start_school_year" jdbcType="OTHER"/>
            <result property="end_school_year" column="end_school_year" jdbcType="OTHER"/>
            <result property="start_term" column="start_term" jdbcType="TINYINT"/>
            <result property="course_condition" column="course_condition" jdbcType="VARCHAR"/>
            <result property="teacher_name" column="teacher_name" jdbcType="VARCHAR"/>
            <result property="teaching_location" column="teaching_location" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        course_id,start_school_year,end_school_year,
        start_term,course_condition,teacher_name,
        teaching_location
