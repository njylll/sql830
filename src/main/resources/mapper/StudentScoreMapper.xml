<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dao.StudentScoreMapper">

    <resultMap id="BaseResultMap" type="com.example.entity.StudentScore">
            <result property="studentId" column="student_id" jdbcType="VARCHAR"/>
            <result property="courseId" column="course_id" jdbcType="VARCHAR"/>
            <result property="usualScore" column="usual_score" jdbcType="FLOAT"/>
            <result property="midTermScore" column="mid_term_score" jdbcType="FLOAT"/>
            <result property="finalTermScore" column="final_term_score" jdbcType="FLOAT"/>
    </resultMap>

    <sql id="Base_Column_List">
        student_id,course_id,usual_score,
        mid_term_score,final_term_score
    </sql>
</mapper>
